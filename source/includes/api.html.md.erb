import requests

url = "https://example.com/brand-report/"

payload = "{\"ad_format\":true,\"ad_types\":{\"include\":[{\"id\":0,\"name\":\"string\"}]},\"api_version\":\"string\",\"countries\":{\"include\":[{\"id\":0,\"name\":\"string\"}]},\"date_range\":{\"start\":\"2019-08-24T14:15:22Z\",\"end\":\"2019-08-24T14:15:22Z\",\"group_by\":\"string\"},\"devices\":{\"include\":[{\"id\":0,\"name\":\"string\"}]},\"new_campaigns\":true,\"order_by\":\"string\",\"sold_by\":{\"include\":[{\"id\":\"string\",\"name\":\"string\"}]},\"websites\":{\"include\":[{\"id\":0,\"domain\":\"string\",\"share\":{\"start\":0,\"end\":0}}],\"exclude\":[{\"id\":0,\"domain\":\"string\",\"share\":{\"start\":0,\"end\":0}}]},\"brands\":{\"include\":[{\"id\":0,\"name\":\"string\"}]}}"
headers = {
    'Content-Type': "application/json",
    'Accept': "application/json",
    'Authorization': "Bearer {access-token}"
    }

